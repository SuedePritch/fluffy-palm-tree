<div class="container" id="'form">
    <form class="form" id="post-form">
            <input type="text" id="title" placeholder="Title Here!" />
            <input type="text" id="projectdescription" placeholder="Description Here!" />
            <input type="text" id="budget" placeholder="Budget Here!" />
            <input type="date" id="deadline" value="0000-00-00" />

            <fieldset id="skills-list">
                {{#each tags as |tag|}}
                    <div id="radio-group">
                        <input type="checkbox" name="skills-list" value={{tag.id}}>
                        <img src={{tag.tech_icon}} id="tech-icon-large">
                    </div>
                {{/each}}
            </fieldset>
            
            <button type="submit">Post!</button>
    </form>
</div>

<script src="/javascript/postform.js"></script>